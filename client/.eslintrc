{
  "extends": ["react-app", "plugin:jsx-a11y/recommended", "airbnb-base"],
  "plugins": ["jsx-a11y"],
  "globals": {
    "System": true
  },
  "env": {
    "node": true
  },
  "rules": {
    "max-len": [
      "error",
      120,
      2,
      {
        "ignoreUrls": true,
        "ignoreComments": false,
        "ignoreRegExpLiterals": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }
    ]
  }
}
